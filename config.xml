<?xml version='1.0' encoding='utf-8'?>
<widget id="com.kadek.onyxreceiver" android-packageName="com.kadek.onyxreceiver" ios-CFBundleIdentifier="com.mitrais.itvisiononyxreceiver" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>OnyxReceiver</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="com-cordova-ble-clientId" value="cd8d429b30f58c577f2bc0dbfd07d03b07de7d2f" />
        <preference name="com-cordova-ble-secret" value="ce7ada82d2b3917dd35520747bf6492b4f5e1d4c" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <preference name="com-cordova-ble-clientId" value="4fbfdad0867f7672a86bd99da0fbc9dc4a00eb28" />
        <preference name="com-cordova-ble-secret" value="c976cb1b16788d27f106a2e3affe2b963072b18a" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    
</widget>
